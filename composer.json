{
  "name": "little-superman/laravel-request-params",
  "description": "Processing request parameters",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "jian Gao",
      "email": "gaojianban@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "illuminate/config": "5.8.*",
    "illuminate/database": "5.8.*",
    "illuminate/support": "5.8.*",
    "illuminate/routing": "5.8.*",
    "symfony/routing": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "LittleSuperman\\RequestParams\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LittleSuperman\\RequestParams\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "LittleSuperman\\RequestParams\\Providers\\ConsoleServiceProvider",
        "LittleSuperman\\RequestParams\\Providers\\ConfigServiceProvider"
      ]
    }
  }
}
